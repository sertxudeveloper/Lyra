{
    "name": "sertxudeveloper/lyra",
    "description": "Lyra is an administration panel for Laravel for bootstrap your app and turn ideas into reality.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "admin",
        "panel",
        "lyra",
        "sertxudeveloper"
    ],
    "support": {
        "issues": "https://github.com/sertxudeveloper/Lyra/issues",
        "source": "https://github.com/sertxudeveloper/Lyra"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\Lyra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SertxuDeveloper\\Lyra\\Tests\\": "tests/",
            "SertxuDeveloper\\Lyra\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "suggest": {
        "sertxudeveloper/laravel-translatable": "Needed to add multilanguage support"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\Lyra\\LyraServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Sertxu Developer",
            "email": "dev.sertxu@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "format": "vendor/bin/pint -v"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.5"
    },
    "prefer-stable": true
}
